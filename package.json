{
  "homepage": "https://dogukanarslan.github.io/shopping-cart/",
  "name": "shopping-cart",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@vitejs/plugin-react": "^4.1.0",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.2.0",
    "vite": "^4.5.0"
  },
  "scripts": {
    "deploy": "gh-pages -d build",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "npx prettier . --write"
  },
  "eslintConfig": {
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:react/jsx-runtime"
    ],
    "rules": {
      "react/prop-types": 0
    },
    "plugins": [
      "react",
      "react-hooks"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.35.0",
    "gh-pages": "^3.1.0",
    "prettier": "3.2.5",
    "vite-plugin-eslint": "^1.8.1"
  }
}
